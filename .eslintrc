{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "max-len": [
      "error",
      {
        "code": 140
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "import/prefer-default-export": "off",
    "no-console": "off",
    "semi": "error"
  }
}